# Model Provider Configuration
# Choose "gemini" (default, uses Google Gemini API) or "ollama" (local models)
MODEL_PROVIDER=gemini

# Google Gemini API Configuration
# Required when MODEL_PROVIDER=gemini
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# LangSmith Configuration (Optional - for observability/tracing)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=deep-research-v1-proto

# Tavily API Configuration (Required - for web search)
# Get your API key from: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# Langfuse Configuration (Optional - alternative observability platform)
# Get your keys from: https://cloud.langfuse.com
# Langfuse provides LLM observability, tracing, and analytics
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com
